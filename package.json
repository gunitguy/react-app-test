{
  "name": "react-app-test",
  "version": "1.0.0",
  "description": "",
  "default": "build/index.html",
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore",
    "lint:fix": "eslint --fix --ignore-path .eslintignore",
    "test": "jest",
    "clean": "npm run clean:build && npm run clean:cache && npm run clean:dist",
    "clean:build": "rm -rf build",
    "clean:cache": "rm -rf .parcel-cache",
    "clean:dist": "rm -rf dist",
    "start": "parcel serve ./src/index.html",
    "prestart": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gunitguy/react-app-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gunitguy/react-app-test/issues"
  },
  "homepage": "https://github.com/gunitguy/react-app-test#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.1",
    "history": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
